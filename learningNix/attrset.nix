{
    string  = "hello"; 
    integer = 1; 
    float   = 4.1; 
    bool    = true; 
    list    = [1 "two" false];
    attribute-set = {
        a = "hello"; 
        b = 2; 
        c = 2.1; 
        d = false; 
        
    }; 
   # as you can see, 
   # These are name-value-pairs. 
   # attribute names do not usually need quotes. 
   # list elements are separated by white space. 
}